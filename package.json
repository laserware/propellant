{
  "name": "tag-parser",
  "version": "1.0.0",
  "description": "GitHub Action that extracts Semver version tag which can be used as npm tag",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "node --test",
    "prettier": "prettier --write index.js parse.js parse.test.js"
  },
  "engines": {
    "node": ">=22"
  },
  "keywords": [
    "github-action",
    "tag",
    "semver"
  ],
  "author": "Mike Rourke <mike.w.rourke@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.0"
  },
  "devDependencies": {
    "prettier": "^3.4.2"
  },
  "prettier": {
    "overrides": [
      {
        "files": [
          "*.test.js"
        ],
        "options": {
          "printWidth": 100
        }
      }
    ]
  }
}
